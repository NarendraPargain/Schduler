      <smtp from="test@gmail.com">
        <network defaultCredentials="false" enableSsl="true" host="smtp.gmail.com" port="587" password="XXXX" userName="test@gmail.com"  targetName="Name"/>
      </smtp>
    </mailSettings>
  </system.net>
  
