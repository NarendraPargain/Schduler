        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

  <connectionStrings>
    <add name="Dbconnection"  connectionString="Server=localhost; Database=SampleDB;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>

  <system.net>
